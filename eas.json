{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development",
        "androidClientId":
        "306687462640-ffl2m3nbom4qt2q3p25q7oal79f51jee.apps.googleusercontent.com",
        "iosClientId":
        "306687462640-ttdagd2ehm3h6asuc185s9til6kaag89.apps.googleusercontent.com",
        "webClientId":
        "306687462640-u3bhdth2p9gqboq44auhacme79rlhekc.apps.googleusercontent.com"
      }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "credentials.json",
        "track": "internal"
      }
    }
  }
}
